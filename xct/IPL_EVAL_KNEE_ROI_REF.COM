$!
$   FILENAME = F$PARSE("''P1'",,,"NAME")
$   DEV      = F$PARSE("''P1'",,,"DEVICE")
$   DIR      = F$PARSE("''P1'",,,"DIRECTORY")
$!
$   BASE_CODE = P2
$!
$   COM_TRAB := DISK4:[BONELAB.PROJECTS.AUTOKNEE.COM]IPL_UPAT_TRAB_REF.COM
$   COM_CORT := DISK4:[BONELAB.PROJECTS.AUTOKNEE.COM]IPL_UPAT_CORT_REF.COM
$   LOG_DIR  := DISK4:[BONELAB.PROJECTS.AUTOKNEE.LOGS.REF]
$!
$   BONE = F$EXTRACT(F$LOCATE(".",FILENAME) - 1, F$LOCATE(".",FILENAME), FILENAME)
$!
$!
$!
$   N = 0
$!
$!
$   DEFINE_ROI_NAMES:
$!
$   GROUP_INT = N + BASE_CODE
