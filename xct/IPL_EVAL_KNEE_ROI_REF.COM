$!
$   ROI_FLAG := _ROI'GROUP_STR'
$!
$!  Here are files needed for the advanced analysis for XT2.
$!
$   ROI_LOG            :=  'LOG_DIR''FILENAME''ROI_FLAG'.LOG
$!
$   IF (N .LE. 5)
$      THEN
$         SUBMIT/QUEUE=SYS$FAST/LOG='ROI_LOG'/PRIORITY=20/NOPRINT/NONOTIFY 'COM_TRAB' /PARAMETERS=('P1', 'ROI_FLAG')
$      ELSE
$         SUBMIT/QUEUE=SYS$FAST/LOG='ROI_LOG'/PRIORITY=20/NOPRINT/NONOTIFY 'COM_CORT' /PARAMETERS=('P1', 'ROI_FLAG')
$   ENDIF
$!
$   N  = N + 1
$   GROUP_INT = GROUP_INT + 1
$   IF (N .LT. 8) THEN GOTO DEFINE_ROI_NAMES
$!
$   EXIT
